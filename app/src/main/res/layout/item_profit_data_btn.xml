<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <data>
        <variable
            name="child"
            type="com.yunshang.haile_manager_android.data.rule.IMultiSelectBottomItemEntity" />
    </data>

    <com.yunshang.haile_manager_android.ui.view.ClickCheckBox
        android:id="@+id/cb_profit_data_btn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:button="@null"
        android:padding="8dp"
        android:gravity="center"
        android:text="@{child.title}"
        android:checked="@{child.check}"
        android:background="@drawable/selector_f7f7f7_bg26strf0a258_r4"
        android:textColor="@color/common_bottom_sheet_item"
        android:textSize="14sp"
        tools:text="首页收益" />
</layout>