<?xml version="1.0" encoding="utf-8"?>
<com.yunshang.haile_manager_android.ui.view.UpTriangleBackgroundLinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="109dp"
    android:layout_height="140dp"
    android:orientation="vertical">

    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/tv_coupon_operate_issue"
        style="@style/Font_14_333333"
        android:layout_width="match_parent"
        android:layout_height="44dp"
        android:drawableStart="@mipmap/icon_device_operate_add"
        android:drawablePadding="4dp"
        android:gravity="center_vertical"
        android:paddingHorizontal="16dp"
        android:text="@string/issue_coupons" />

    <View
        style="@style/DividingStyle"
        android:layout_marginHorizontal="12dp" />

    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/tv_coupon_operate_batch_invalid"
        style="@style/Font_14_333333"
        android:layout_width="match_parent"
        android:layout_height="44dp"
        android:drawableStart="@mipmap/icon_coupon_invalid"
        android:drawablePadding="4dp"
        android:gravity="center"
        android:paddingHorizontal="16dp"
        android:text="@string/batch_invalid" />

</com.yunshang.haile_manager_android.ui.view.UpTriangleBackgroundLinearLayout>